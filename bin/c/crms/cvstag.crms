#!/l/local/bin/perl


my $rootdir = '/l1';

# These are class-specific directories or directories which always
# span classes such as /l1/bin

my %dirs = ( 
            '/bin/c/crms/'   => '-R', 
            '/cgi/c/crms/'   => '-R', 
            '/web/c/crms/'   => '-R', 
           );

  
require $ENV{'DLXSROOT'} . '/bin/CvstagHelper.pm';

my $tag = $ARGV[0];
my $opt = $ARGV[1];

CvstagHelper::Tag($rootdir, \%dirs, $tag, $opt);
exit 0;
