<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Strict//EN">
<html>
  <head>
    <link rel="stylesheet" type="text/css" href="/c/crms/css/review.css" />
    <title>[% title %]</title>
    <script language="javascript">
// Colorizes all the userRateTable type tables in the document
function init()
{
  var colors = ["#CCFFFF", "#FFFFFF"];
  var tables = document.body.getElementsByTagName("table");
  var ntables = tables.length;
  for (var i = 0; i < ntables; i++)
  {
    var table = tables[i];
    if (table.id == "userRateTable")
    {
      var nrows = table.rows.length;
      for (var j = 0; j < nrows; j++)
      {
        var tr = table.rows[j];
        tr.style.backgroundColor = colors[j % 2];
      }
    }
  }
}
  </script>
  </head>
  <body onload="init()">
  <div class="pages" id="homePage">
    <img src="/c/crms/crms.jpg"/>
    <br/><br/>
    <font size="5"><strong>Hello [% crms.GetUserName %] </strong></font>
    <a href="crms?p=Logout"> Logout</a>
    [% FOREACH error IN crms.GetErrors %]
      <strong>ERROR: [% error %]</strong>
      <br/>
    [% END %]
<br/>
<br/>

