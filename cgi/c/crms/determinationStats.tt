    [% page = cgi.param('p') %]
    [% dev = crms.get('dev') %]
    [% fulltitle = title %]
    [% IF dev %]
      [% fulltitle = fulltitle _ ' - ' _ 'Development' %]
      [% IF dev != 1 %]
        [% fulltitle = fulltitle _ ' (' _ dev _ ')' %]
      [% ELSE %]
      [% END %]
    [% END %]
    <title>[% fulltitle %] - [% crms.PageToEnglish(page) %]</title>
