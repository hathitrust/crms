    <script language="JavaScript" src="/c/crms/swfobject.js"></script>
    <script language="javascript">

// For Open Flash Charts Data
function findSWF(id)
{
  if (navigator.appName.indexOf("Microsoft")!= -1) return window[id];
  else return document[id];
}

function SetOFCData(json,name)
{
  swf = findSWF(name);
  x = swf.load(json);
}

function DoChart(cumulative,id)
{
  var url = "/cgi/c/crms/getExportStats?c=" + cumulative;
  var req = null;
  if (window.XMLHttpRequest) req = new XMLHttpRequest();
  else if (window.ActiveXObject) 
  {
    try {
      req = new ActiveXObject("Msxml2.XMLHTTP");
    } catch (e)
    {
      try {
        req = new ActiveXObject("Microsoft.XMLHTTP");
      } catch (e) {}
    }
  }
  req.onreadystatechange = function()
  {
    if (req.readyState == 4)
    {
      if (req.status == 200) SetOFCData(req.responseText, id);
    }
  };
  req.open("GET", url, true);
  req.send(null);
}

swfobject.embedSWF("/c/crms/open-flash-chart.swf", "chart0", "400", "300", "9.0.0",
                   "expressInstall.swf", {"data-file":escape("/cgi/c/crms/getExportStats?type=graph&c=0")});
swfobject.embedSWF("/c/crms/open-flash-chart.swf", "chart1", "400", "300", "9.0.0",
                   "expressInstall.swf", {"data-file":escape("/cgi/c/crms/getExportStats?type=graph&c=1")});
swfobject.embedSWF("/c/crms/open-flash-chart.swf", "chart2", "400", "300", "9.0.0",
                   "expressInstall.swf", {"data-file":escape("/cgi/c/crms/getExportStats?type=graph&c=2")});
</script>
